{ 
  "version": 1,
  "formatters": { 
    "standard": { 
      "format": "%(asctime)s %(levelname)-8s %(message)s"
    }
  },
  "handlers": { 
    "console": {
      "level": "DEBUG",
      "formatter": "standard",
      "class": "logging.StreamHandler",
      "stream": "ext://sys.stdout"
    },
    "rotation": { 
      "level": "DEBUG",
      "formatter": "standard",
      "filename": "logs/enms.log",
      "class": "logging.handlers.RotatingFileHandler",
      "maxBytes": 20000000,
      "backupCount": 10
    }
  },
  "loggers": { 
    "": {
      "handlers": ["console", "rotation"],
      "level": "DEBUG"
    }
  } 
}